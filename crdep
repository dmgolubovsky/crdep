#! /bin/bash

# crdep: create and manage VM images for Chrome Remote Desktop EndPoints
# pre-script

# The crdep script is distributed as a SHAR self-unpacking archive. A user is expected to place it in any
# location in their $PATH. When downloaded first it looks in the current user's home directory for a subdirectory 
# named .crdep and, if such subdirectory does not exist, creates it and extracts the SHAR'ed files there. 
# If such subdirectory exists it looks for a shell script located in $HOME/.crdep/scripts/crdep-main.sh and executes it
# for any further action. Therefore, to reinstall the whole tool, just the $HOME/.crdep subdrectory has to be removed,
# and the script re-downloaded.

# Look if the target script exists and if not, unshar itself there.

export CRDEPDIR="$HOME/.crdep" 

[ -x "$CRDEPDIR/scripts/crdep-main.sh" ] && {
  "$CRDEPDIR/scripts/crdep-main.sh" "$@"
  exit 0
} || {
  mkdir -p "$CRDEPDIR"
  cd "$CRDEPDIR"

# shar contents goes here and the closing bracket added by Makefile #


#!/bin/sh
# This is a shell archive (produced by GNU sharutils 4.15.2).
# To extract the files from this archive, save it to some FILE, remove
# everything before the '#!/bin/sh' line above, then type 'sh FILE'.
#
lock_dir=_sh2480658
# Made on 2023-01-07 13:06 EST by <golubovsky@optiplex>.
# Source directory was '/home/golubovsky/src/crdep'.
#
# Existing files will *not* be overwritten, unless '-c' is specified.
#
# This shar contains:
# length mode       name
# ------ ---------- ------------------------------------------
#   4173 -rw-rw-r-- docker/crdep-base.Dockerfile
#    522 -rw-rw-r-- docker/kernel.Dockerfile
#    328 -rw-rw-r-- docker/init.Dockerfile
#  10508 -rwxrwxr-x scripts/crdep-main.sh
#  84687 -rw-rw-r-- kernel/linux-5.15.85-config
#    176 -rw-rw-r-- xtrafiles/units/conlog.service
#    128 -rw-rw-r-- xtrafiles/units/dhclient@.service
#     98 -rw-rw-r-- xtrafiles/fsmod.conf
#    615 -rwxrwxr-x xtrafiles/squash
#   2961 -rw-rw-r-- xtrafiles/org.freedesktop.timedate1.policy
#    557 -rw-rw-r-- xtrafiles/issue
#    114 -rw-rw-r-- xtrafiles/nolock.desktop
#     47 -rw-rw-r-- xtrafiles/jl.conf
#    369 -rw-rw-r-- xtrafiles/45-allow-colord.pkla
#    190 -rw-r--r-- xtrafiles/hosts
#    861 -rw-rw-r-- xtrafiles/init.c
#    353 -rwxrwxr-x xtrafiles/nolock.sh
#    711 -rw-rw-r-- xtrafiles/bash_login
#    599 -rwxrwxr-x xtrafiles/crd-auth
#    708 -rwxrwxr-x xtrafiles/overlay
#
MD5SUM=${MD5SUM-md5sum}
f=`${MD5SUM} --version | egrep '^md5sum .*(core|text)utils'`
test -n "${f}" && md5check=true || md5check=false
${md5check} || \
  echo 'Note: not verifying md5sums.  Consider installing GNU coreutils.'
if test "X$1" = "X-c"
then keep_file=''
else keep_file=true
fi
echo=echo
save_IFS="${IFS}"
IFS="${IFS}:"
gettext_dir=
locale_dir=
set_echo=false

for dir in $PATH
do
  if test -f $dir/gettext \
     && ($dir/gettext --version >/dev/null 2>&1)
  then
    case `$dir/gettext --version 2>&1 | sed 1q` in
      *GNU*) gettext_dir=$dir
      set_echo=true
      break ;;
    esac
  fi
done

if ${set_echo}
then
  set_echo=false
  for dir in $PATH
  do
    if test -f $dir/shar \
       && ($dir/shar --print-text-domain-dir >/dev/null 2>&1)
    then
      locale_dir=`$dir/shar --print-text-domain-dir`
      set_echo=true
      break
    fi
  done

  if ${set_echo}
  then
    TEXTDOMAINDIR=$locale_dir
    export TEXTDOMAINDIR
    TEXTDOMAIN=sharutils
    export TEXTDOMAIN
    echo="$gettext_dir/gettext -s"
  fi
fi
IFS="$save_IFS"
if (echo "testing\c"; echo 1,2,3) | grep c >/dev/null
then if (echo -n test; echo 1,2,3) | grep n >/dev/null
     then shar_n= shar_c='
'
     else shar_n=-n shar_c= ; fi
else shar_n= shar_c='\c' ; fi
f=shar-touch.$$
st1=200112312359.59
st2=123123592001.59
st2tr=123123592001.5 # old SysV 14-char limit
st3=1231235901

if   touch -am -t ${st1} ${f} >/dev/null 2>&1 && \
     test ! -f ${st1} && test -f ${f}; then
  shar_touch='touch -am -t $1$2$3$4$5$6.$7 "$8"'

elif touch -am ${st2} ${f} >/dev/null 2>&1 && \
     test ! -f ${st2} && test ! -f ${st2tr} && test -f ${f}; then
  shar_touch='touch -am $3$4$5$6$1$2.$7 "$8"'

elif touch -am ${st3} ${f} >/dev/null 2>&1 && \
     test ! -f ${st3} && test -f ${f}; then
  shar_touch='touch -am $3$4$5$6$2 "$8"'

else
  shar_touch=:
  echo
  ${echo} 'WARNING: not restoring timestamps.  Consider getting and
installing GNU '\''touch'\'', distributed in GNU coreutils...'
  echo
fi
rm -f ${st1} ${st2} ${st2tr} ${st3} ${f}
#
if test ! -d ${lock_dir} ; then :
else ${echo} "lock directory ${lock_dir} exists"
     exit 1
fi
if mkdir ${lock_dir}
then ${echo} "x - created lock directory ${lock_dir}."
else ${echo} "x - failed to create lock directory ${lock_dir}."
     exit 1
fi
# ============= docker/crdep-base.Dockerfile ==============
if test ! -d 'docker'; then
  mkdir 'docker'
if test $? -eq 0
then ${echo} "x - created directory docker."
else ${echo} "x - failed to create directory docker."
     exit 1
fi
fi
