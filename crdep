${echo} xz-decompressing file scripts/crdep-main.sh &&

xz -dc ${lock_dir}/xzi > 'scripts/crdep-main.sh' && \
  (set 20 23 01 07 13 06 23 'scripts/crdep-main.sh'
   eval "${shar_touch}") && \
  chmod 0775 'scripts/crdep-main.sh'
if test $? -ne 0
then ${echo} "restore of scripts/crdep-main.sh failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'scripts/crdep-main.sh': 'MD5 check failed'
       ) << \SHAR_EOF
8ad6282ef48bc17f3fdf93562f18bff1  scripts/crdep-main.sh
SHAR_EOF

else
test `LC_ALL=C wc -c < 'scripts/crdep-main.sh'` -ne 10508 && \
  ${echo} "restoration warning:  size of 'scripts/crdep-main.sh' is not 10508"
  fi
fi
