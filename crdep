${echo} xz-decompressing file kernel/linux-5.15.85-config &&

xz -dc ${lock_dir}/xzi > 'kernel/linux-5.15.85-config' && \
  (set 20 22 12 31 13 58 29 'kernel/linux-5.15.85-config'
   eval "${shar_touch}") && \
  chmod 0664 'kernel/linux-5.15.85-config'
if test $? -ne 0
then ${echo} "restore of kernel/linux-5.15.85-config failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'kernel/linux-5.15.85-config': 'MD5 check failed'
       ) << \SHAR_EOF
b87fc86fbea0eb802ced2eaca23c4efd  kernel/linux-5.15.85-config
SHAR_EOF

else
test `LC_ALL=C wc -c < 'kernel/linux-5.15.85-config'` -ne 84687 && \
  ${echo} "restoration warning:  size of 'kernel/linux-5.15.85-config' is not 84687"
  fi
fi
# ============= xtrafiles/units/conlog.service ==============
if test ! -d 'xtrafiles'; then
  mkdir 'xtrafiles'
if test $? -eq 0
then ${echo} "x - created directory xtrafiles."
else ${echo} "x - failed to create directory xtrafiles."
     exit 1
fi
fi
if test ! -d 'xtrafiles/units'; then
  mkdir 'xtrafiles/units'
if test $? -eq 0
then ${echo} "x - created directory xtrafiles/units."
else ${echo} "x - failed to create directory xtrafiles/units."
     exit 1
fi
fi
if test -n "${keep_file}" && test -f 'xtrafiles/units/conlog.service'
then
${echo} "x - SKIPPING xtrafiles/units/conlog.service (file already exists)"

else
${echo} "x - extracting xtrafiles/units/conlog.service xz-compressed"
  sed 's/^X//' << 'SHAR_EOF' | uudecode &&
begin 600 _sh2480658/xzi
M_3=Z6%H```3FUK1&`@`A`1P````0SUC,X`"O`*%=`"V52C5:TT]<GQQP2AQC
ML`;T+9&-O.=O+FX9+<CC^NYHDM;I*+J4073H,]-;0'?S[7P<+N"5QCB;X;\U
M5PBHX,$U+5.1_XU@H,_8=JY(DU`*Z`K'G#D%5HZ(%0GF""6.TR4WRG8'T5A?
MQ^R\_)=$SV1[V.3F]'MO*T&C2@ERE_LF?E(IO)<M]L*G!*LC>R.,16J\-"$Q
MLH,W]N;@:J%TLV4``````/IC(]CM<O<\``&]`;`!``!KV=%$L<1G^P(`````
#!%E:
`
end
